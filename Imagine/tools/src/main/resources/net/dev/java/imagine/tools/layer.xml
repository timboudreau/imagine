<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN" "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="brushes">
        <attr name="displayName" bundlevalue="org.netbeans.paint.tools.Bundle#brushes"/>
    </folder>
    <folder name="fills">
        <attr name="displayName" bundlevalue="org.netbeans.paint.tools.Bundle#fills"/>
        <file name="TestFill.instance" url="resources/smudge.png">
            <attr name="SystemFileSystem.icon" urlvalue="nbfs:/SystemFileSystem/fills/TestFill.instance"/>
            <attr name="instanceClass" stringvalue="org.netbeans.paint.tools.fills.PatternFill"/>
            <attr name="instanceCreate" methodvalue="org.netbeans.paint.tools.fills.PatternFill.create"/>
            <attr name="position" intvalue="450"/>
        </file>
    </folder>
    <folder name="pathcreators">
        <attr name="displayName" bundlevalue="org.netbeans.paint.tools.Bundle#pathcreators"/>
    </folder>

    <folder name="brushtips">
        <attr name="displayName" bundlevalue="org.netbeans.paint.tools.Bundle#brushtips"/>
    </folder>
</filesystem>
